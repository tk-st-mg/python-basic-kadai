        "import random\n",
        "\n",
        "var = random.randint(0,1000)\n",
        "\n",
        "if var % 3 == 0 and var % 5 == 0 and var != 0:\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0 and var != 0:\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0 and var != 0:\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    print(var)"
      ]
